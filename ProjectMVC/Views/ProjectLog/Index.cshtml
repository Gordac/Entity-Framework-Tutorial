@model IEnumerable<ProjectMVC.Models.ProjectLog>

@{
    ViewBag.Title = "Index";
}

<h2>Project Log</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.LogID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ProjectID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ManagerID)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.LogID)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.ProjectID)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.ManagerID)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.LogID }) |
        @Html.ActionLink("Details", "Details", new { id = item.LogID }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.LogID })
    </td>
</tr>
}

</table>
